{
  "serviceName": "redis-cache",
  "taskDefinition": "redis-cache",
  "cluster": "your-cluster-name",
  "launchType": "FARGATE",
  "desiredCount": 2,
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100
  },
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
